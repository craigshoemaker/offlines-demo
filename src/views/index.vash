@html.extend('layout', function(model){

	@html.block('body', function(model){
	
		@if(model.error){
			<p class="alert alert-danger">@model.error</p>
		}

        <div class="list">
		    @model.parks.forEach(function(park){
                    <a href="/parks/@park.nameEncoded"><div class="h3">@park.name</div></a>
		    })
        </div>
    })
})